{
  "name": "SheetySite",
  "private": true,
  "description": "Create listing websites with Google Sheets data",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "firebase": "^7.14.6",
    "fuse.js": "^5.2.3",
    "gatsby": "^2.21.0",
    "gatsby-plugin-google-gtag": "^2.1.1",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-postcss": "^2.3.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-source-google-spreadsheets": "^3.0.5",
    "lint-staged": "^10.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "typescript": "^3.8.3",
    "use-react-modal": "^0.0.9"
  },
  "devDependencies": {
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^5.14",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tengkuhafidz/sheetysite/"
  },
  "bugs": {
    "url": "https://github.com/tengkuhafidz/sheetysite/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:fix"
    ]
  }
}
